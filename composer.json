{
    "name": "wijourdil/project-setup",
    "description": "This package provides a command to install and configure automatically all the useful packages for a Laravel / Lumen project",
    "keywords": [
        "laravel",
        "lumen",
        "project",
        "setup",
        "install"
    ],
    "homepage": "https://github.com/wijourdil/project-setup",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wijourdil\\ProjectSetup\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wijourdil\\ProjectSetup\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Wilfried Jourdil",
            "email": "wijourdil@protonmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0",
        "illuminate/support": "^10.0",
        "ext-readline": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "nunomaduro/larastan": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wijourdil\\ProjectSetup\\ProjectSetupServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
